    "animation.mdrowned.look_at_target": {
      "loop": true,
      "bones": {
        "head": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this", 0.0 ]
        },
        "body": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation * 0.25", "query.target_y_rotation * 0.25", 0.0 ]
        },
        

        
        "topbody": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation * 0.25", "query.target_y_rotation * 0.25", 0.0 ]
        }
      }
    },
