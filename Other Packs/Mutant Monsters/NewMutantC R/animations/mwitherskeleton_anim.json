  "format_version": "1.8.0",
  "animations": {
    "animation.mwitherskeleton.look_at_target": {
      "loop": true,
      "bones": {
        "head": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this", 0.0 ]
        },
        "neck": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation * 0.25", "query.target_y_rotation * 0.25", 0.0 ]
        }
        


      }
    },
    "animation.mwitherskeleton.look_at_target_head": {
      "loop": true,
      "bones": {
           "head": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this", 0.0 ]
        },
        "neck": {
          "relative_to": { "rotation": "entity" },
          "rotation": [ "query.target_x_rotation * 0.25", "query.target_y_rotation * 0.25", 0.0 ]
        }
      }
    },
    

    "animation.mwitherskeleton.attack_bare_hand": {
      "loop": true,
      "bones": {
        "rightarm": {
          "rotation": [
            "Math.sin(1.0 - Math.pow(1.0 - variable.attack_time, -10.0) * 180.0) * (variable.is_brandishing_spear ? -1.0 : 1.0 )",
            "variable.is_brandishing_spear ? 0.0 : variable.attack_body_rot_y * 2.0",
            0.0
          ]
        },
        "leftarm": {
          "rotation": [
            "Math.sin(1.0 - Math.pow(1.0 - variable.attack_time, 10.0) * 180.0) * (variable.is_brandishing_spear ? -1.0 : 1.0 )",
            "variable.is_brandishing_spear ? 0.0 : variable.attack_body_rot_y * -2.0",
            0.0
          ]
        }

      }
    },  
    
    
