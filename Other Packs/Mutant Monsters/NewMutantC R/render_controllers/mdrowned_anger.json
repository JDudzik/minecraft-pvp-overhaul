{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.mdrowned_anger": {
      "geometry": "Geometry.glint",

      "materials": [ { "*": "Material.glint" } ],
      "textures": [ "Texture.glint" ],

      "part_visibility": [ { "*": "query.variant == 1" } ],
      "uv_anim": {
        "offset": [
          "(query.variant == 1 ? (Math.cos(query.life_time * 0) * 0) : 0.0)",
          "(query.variant == 1 ? (Math.cos (query.life_time * 0) * 0) : 0.0)"
        ],
          "scale": [ 1.0, 1.0 ]
      },
      "light_color_multiplier": 0.5,
      "ignore_lighting": true
    }
  }
}


