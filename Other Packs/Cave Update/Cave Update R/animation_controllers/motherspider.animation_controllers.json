{
  "format_version": "1.8.0",
  "animation_controllers": {
    "controller.animation.spidermother.move": {
      "states": {
        "default": {
          "parameters": [ "query.modified_move_speed" ],
          "animations": [
            { "default_leg_pose": [] },
            {
              "walk": [
                {
                  "0.0": 0.0,
                  "1.0": 1.0
                }
              ]
            },
            { "look_at_target": [] }
           ],
          "transitions": [
            { "casting": "query.is_casting" }
          ]
        },
        "casting": {
          "animations": [
            { "look_at_target": [] },            { "default_leg_pose": [] },
            { "shaking": [] }
          ],
          "particle_effects": [
            {
              "effect": "spell",
              "locator": "left_hand"
            },
            {
              "effect": "spell",
              "locator": "right_hand"
	    }
          ],
          "transitions": [
            { "default": "!query.is_casting" }
          ]
        }
      }
    }
  }
}
